if not _G.FL then 
	_G.FL = true
	local Label
	for index, obj in game.Players.LocalPlayer.PlayerGui.WaitForChild("Anim", math.huge):GetChildren() do
		if obj:IsA("TextLabel") then
			if string.find(obj.Text, "Онлайн") then
				obj.LocalScript.Disabled = true
				Label = obj
			end
		end
	end
	workspace:WaitForChild("MainTimerForNew", math.huge):WaitForChild("Part", math.huge):WaitForChild("SurfaceGui", math.huge):WaitForChild("TextLabel", math.huge):Changed:Connect(function()
		Label.Text = workspace.MainTimerForNew.Part.SurfaceGui.TextLabel.Text
	end)
end
